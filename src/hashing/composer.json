{
    "name": "hypervel/hashing",
    "type": "library",
    "description": "The hashing package for Hypervel.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "hashing",
        "swoole",
        "hypervel"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@hypervel.org"
        }
    ],
    "support": {
        "issues": "https://github.com/hypervel/components/issues",
        "source": "https://github.com/hypervel/components"
    },
    "autoload": {
        "psr-4": {
            "Hypervel\\Hashing\\": "src/"
        }
    },
    "require": {
        "php": "^8.4",
        "hypervel/context": "^0.4",
        "hyperf/support": "~3.1.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hypervel": {
            "providers": [
                "Hypervel\\Hashing\\HashingServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "0.4-dev"
        }
    }
}