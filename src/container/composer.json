{
    "name": "hypervel/container",
    "description": "The Container package for Hypervel.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "container",
        "swoole",
        "hypervel"
    ],
    "support": {
        "issues": "https://github.com/hypervel/components/issues",
        "source": "https://github.com/hypervel/components"
    },
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@hypervel.org"
        },
        {
            "name": "Raj Siva-Rajah",
            "homepage": "https://github.com/binaryfire"
        }
    ],
    "require": {
        "php": "^8.4",
        "hypervel/context": "^0.4",
        "psr/container": "^2.0.2"
    },
    "autoload": {
        "psr-4": {
            "Hypervel\\Container\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.4-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}