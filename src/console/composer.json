{
    "name": "hypervel/console",
    "type": "library",
    "description": "The console package for Hypervel.",
    "license": "MIT",
    "keywords": [
        "php",
        "console",
        "swoole",
        "hypervel"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@hypervel.org"
        }
    ],
    "support": {
        "issues": "https://github.com/hypervel/components/issues",
        "source": "https://github.com/hypervel/components"
    },
    "autoload": {
        "psr-4": {
            "Hypervel\\Console\\": "src/"
        }
    },
    "require": {
        "php": "^8.4",
        "guzzlehttp/guzzle": "^7.8.2",
        "hypervel/cache": "^0.4",
        "hypervel/coroutine": "^0.4",
        "hypervel/foundation": "^0.4",
        "hypervel/queue": "^0.4",
        "dragonmantank/cron-expression": "^3.3.2",
        "hypervel/prompts": "^0.4",
        "symfony/console": "^6.2|^7.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hypervel": {
            "providers": [
                "Hypervel\\Console\\ConsoleServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "0.4-dev"
        }
    }
}