{
    "name": "hypervel/websocket-server",
    "type": "library",
    "description": "The Hypervel WebSocket Server package.",
    "license": "MIT",
    "keywords": [
        "php",
        "websocket",
        "swoole",
        "hypervel"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@hypervel.org"
        },
        {
            "name": "Raj Siva-Rajah",
            "homepage": "https://github.com/binaryfire"
        }
    ],
    "support": {
        "issues": "https://github.com/hypervel/components/issues",
        "source": "https://github.com/hypervel/components"
    },
    "autoload": {
        "psr-4": {
            "Hypervel\\WebSocketServer\\": "src/"
        }
    },
    "require": {
        "php": "^8.4",
        "hyperf/contract": "~3.1.0",
        "hypervel/engine": "^0.4",
        "hypervel/http-message": "^0.4",
        "hypervel/context": "^0.4",
        "hypervel/coordinator": "^0.4",
        "hypervel/coroutine": "^0.4",
        "hypervel/dispatcher": "^0.4",
        "hypervel/event": "^0.4",
        "hypervel/exception-handler": "^0.4",
        "hypervel/framework": "^0.4",
        "hypervel/http-server": "^0.4",
        "hypervel/server": "^0.4",
        "hypervel/support": "^0.4",
        "psr/http-message": "^2.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hypervel": {
            "providers": [
                "Hypervel\\WebSocketServer\\WebSocketServerServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "0.4-dev"
        }
    }
}
