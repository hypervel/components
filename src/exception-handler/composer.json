{
    "name": "hypervel/exception-handler",
    "type": "library",
    "description": "The Hypervel Exception Handler package.",
    "license": "MIT",
    "keywords": [
        "php",
        "exception-handler",
        "swoole",
        "hypervel"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@hypervel.org"
        },
        {
            "name": "Raj Siva-Rajah",
            "homepage": "https://github.com/binaryfire"
        }
    ],
    "support": {
        "issues": "https://github.com/hypervel/components/issues",
        "source": "https://github.com/hypervel/components"
    },
    "autoload": {
        "psr-4": {
            "Hypervel\\ExceptionHandler\\": "src/"
        }
    },
    "require": {
        "php": "^8.4",
        "hyperf/contract": "~3.1.0",
        "hypervel/context": "^0.4",
        "hypervel/dispatcher": "^0.4",
        "hypervel/event": "^0.4",
        "hypervel/support": "^0.4",
        "psr/container": "^1.0|^2.0",
        "psr/http-message": "^2.0"
    },
    "suggest": {
        "filp/whoops": "Required to use WhoopsExceptionHandler (^2.15)"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "Hypervel\\ExceptionHandler\\ConfigProvider"
        },
        "branch-alias": {
            "dev-main": "0.4-dev"
        }
    }
}
