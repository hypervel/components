{
    "name": "hypervel/encryption",
    "type": "library",
    "description": "The encryption package for Hypervel.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "encryption",
        "swoole",
        "hypervel"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@hypervel.org"
        }
    ],
    "support": {
        "issues": "https://github.com/hypervel/components/issues",
        "source": "https://github.com/hypervel/components"
    },
    "autoload": {
        "psr-4": {
            "Hypervel\\Encryption\\": "src/"
        }
    },
    "require": {
        "php": "^8.4",
        "ext-hash": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hypervel": {
            "providers": [
                "Hypervel\\Encryption\\EncryptionServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "0.4-dev"
        }
    }
}