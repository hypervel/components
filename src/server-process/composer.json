{
    "name": "hypervel/server-process",
    "type": "library",
    "description": "Manages custom Swoole worker processes for Hypervel.",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "process",
        "server",
        "hypervel"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@hypervel.org"
        },
        {
            "name": "Raj Siva-Rajah",
            "homepage": "https://github.com/binaryfire"
        }
    ],
    "support": {
        "issues": "https://github.com/hypervel/components/issues",
        "source": "https://github.com/hypervel/components"
    },
    "autoload": {
        "psr-4": {
            "Hypervel\\ServerProcess\\": "src/"
        }
    },
    "require": {
        "php": "^8.4",
        "hypervel/contracts": "^0.4",
        "hypervel/coordinator": "^0.4",
        "hypervel/coroutine": "^0.4",
        "hypervel/engine": "^0.4"
    },
    "extra": {
        "hypervel": {
            "providers": [
                "Hypervel\\ServerProcess\\ServerProcessServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "0.4-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
