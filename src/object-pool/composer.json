{
    "name": "hypervel/object-pool",
    "description": "The object pool package for Hypervel.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "pool",
        "object-pool",
        "swoole",
        "hypervel"
    ],
    "support": {
        "issues": "https://github.com/hypervel/components/issues",
        "source": "https://github.com/hypervel/components"
    },
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@hypervel.org"
        }
    ],
    "require": {
        "php": "^8.4",
        "hypervel/coroutine": "^0.4",
        "hypervel/engine": "^0.4",
        "hypervel/coordinator": "^0.4",
        "hyperf/contract": "~3.1.0",
        "hyperf/support": "~3.1.0",
        "nesbot/carbon": "^2.72.6"
    },
    "autoload": {
        "psr-4": {
            "Hypervel\\ObjectPool\\": "src/"
        }
    },
    "extra": {
        "hyperf": {
            "config": "Hypervel\\ObjectPool\\ConfigProvider"
        },
        "branch-alias": {
            "dev-main": "0.4-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}