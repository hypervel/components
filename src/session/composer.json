{
    "name": "hypervel/session",
    "type": "library",
    "description": "The session package for Hypervel.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "session",
        "swoole",
        "hypervel"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@hypervel.org"
        }
    ],
    "support": {
        "issues": "https://github.com/hypervel/components/issues",
        "source": "https://github.com/hypervel/components"
    },
    "autoload": {
        "psr-4": {
            "Hypervel\\Session\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "require": {
        "php": "^8.4",
        "ext-session": "*",
        "hypervel/context": "^0.4",
        "hypervel/collections": "^0.4",
        "hyperf/support": "~3.1.0",
        "hypervel/macroable": "^0.4",
        "hypervel/cache": "^0.4",
        "hypervel/cookie": "^0.4",
        "hypervel/http-server": "^0.4",
        "hypervel/support": "^0.4"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "Hypervel\\Session\\ConfigProvider"
        },
        "branch-alias": {
            "dev-main": "0.4-dev"
        }
    }
}
