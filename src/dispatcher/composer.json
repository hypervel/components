{
    "name": "hypervel/dispatcher",
    "description": "The dispatcher package for Hypervel.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "dispatcher",
        "middleware",
        "swoole",
        "hypervel"
    ],
    "support": {
        "issues": "https://github.com/hypervel/components/issues",
        "source": "https://github.com/hypervel/components"
    },
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@hypervel.org"
        }
    ],
    "require": {
        "php": "^8.4",
        "hypervel/context": "^0.4",
        "hypervel/http-server": "^0.4",
        "hypervel/pipeline": "^0.4"
    },
    "autoload": {
        "psr-4": {
            "Hypervel\\Dispatcher\\": "src/"
        }
    },
    "extra": {
        "hyperf": {
            "config": "Hypervel\\Dispatcher\\ConfigProvider"
        },
        "branch-alias": {
            "dev-main": "0.4-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
