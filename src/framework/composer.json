{
    "name": "hypervel/framework",
    "type": "library",
    "description": "Swoole server lifecycle management for Hypervel.",
    "license": "MIT",
    "keywords": [
        "php",
        "framework",
        "swoole",
        "hypervel"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@hypervel.org"
        },
        {
            "name": "Raj Siva-Rajah",
            "homepage": "https://github.com/binaryfire"
        }
    ],
    "support": {
        "issues": "https://github.com/hypervel/components/issues",
        "source": "https://github.com/hypervel/components"
    },
    "autoload": {
        "psr-4": {
            "Hypervel\\Framework\\": "src/"
        }
    },
    "require": {
        "php": "^8.4",
        "hypervel/contracts": "^0.4",
        "hypervel/coordinator": "^0.4",
        "hypervel/coroutine": "^0.4",
        "psr/log": "^1.0|^2.0|^3.0",
        "symfony/console": "^7.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.4-dev"
        }
    }
}
