{
    "name": "hypervel/cache",
    "type": "library",
    "description": "The cache package for Hypervel.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "cache",
        "swoole",
        "hypervel"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@hypervel.org"
        },
        {
            "name": "Raj Siva-Rajah",
            "homepage": "https://github.com/binaryfire"
        }        
    ],
    "support": {
        "issues": "https://github.com/hypervel/components/issues",
        "source": "https://github.com/hypervel/components"
    },
    "autoload": {
        "psr-4": {
            "Hypervel\\Cache\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "require": {
        "php": "^8.4",
        "hypervel/support": "^0.4",
        "laravel/serializable-closure": "^1.3",
        "psr/simple-cache": "^3.0"
    },
    "suggest": {
        "hypervel/redis": "Required to use redis driver. (^0.4).",
        "hypervel/cache": "Required to use file lock. (^0.4)"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hypervel": {
            "providers": [
                "Hypervel\\Cache\\CacheServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "0.4-dev"
        }
    }
}
