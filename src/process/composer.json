{
    "name": "hypervel/process",
    "description": "The process package for Hypervel.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "process",
        "swoole",
        "hypervel"
    ],
    "support": {
        "issues": "https://github.com/hypervel/components/issues",
        "source": "https://github.com/hypervel/components"
    },
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@hypervel.org"
        }
    ],
    "require": {
        "php": "^8.4",
        "hypervel/support": "^0.4",
        "hypervel/collections": "^0.4",
        "hypervel/macroable": "^0.4",
        "symfony/process": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Hypervel\\Process\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.4-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
