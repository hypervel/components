{
    "name": "hypervel/event",
    "type": "library",
    "description": "The event package for Hypervel.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "event",
        "swoole",
        "hypervel"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@hypervel.org"
        }
    ],
    "support": {
        "issues": "https://github.com/hypervel/components/issues",
        "source": "https://github.com/hypervel/components"
    },
    "autoload": {
        "psr-4": {
            "Hypervel\\Event\\": "src/",
            "Illuminate\\Events\\": "illuminate/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "require": {
        "php": "^8.4",
        "hyperf/event": "~3.1.0",
        "hypervel/collections": "^0.4",
        "hypervel/context": "^0.4",
        "hypervel/bus": "^0.4",
        "laravel/serializable-closure": "^1.3"
    },
    "suggest": {
        "hypervel/queue": "Required to use queueable listener. (~0.1.0)"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "Hypervel\\Event\\ConfigProvider"
        },
        "branch-alias": {
            "dev-main": "0.4-dev"
        }
    }
}
