{
    "name": "hypervel/http-server",
    "type": "library",
    "description": "The Hypervel HTTP Server package.",
    "license": "MIT",
    "keywords": [
        "php",
        "http",
        "server",
        "swoole",
        "hypervel"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@hypervel.org"
        },
        {
            "name": "Raj Siva-Rajah",
            "homepage": "https://github.com/binaryfire"
        }
    ],
    "support": {
        "issues": "https://github.com/hypervel/components/issues",
        "source": "https://github.com/hypervel/components"
    },
    "autoload": {
        "psr-4": {
            "Hypervel\\HttpServer\\": "src/"
        }
    },
    "require": {
        "php": "^8.4",
        "hypervel/contracts": "^0.4",
        "hypervel/coordinator": "^0.4",
        "hypervel/dispatcher": "^0.4",
        "hypervel/engine": "^0.4",
        "hypervel/exception-handler": "^0.4",
        "hypervel/http-message": "^0.4",
        "hypervel/server": "^0.4",
        "psr/http-message": "^2.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.4-dev"
        }
    }
}
